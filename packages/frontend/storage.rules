service firebase.storage {
    match /b/{bucket}/o {
        match /default/{userId}/default {
            allow read: if request.auth != null && request.auth.uid == userId
        }
        match /private/{userId}/{all=**} {
            allow read, delete: if request.auth != null && request.auth.uid == userId
        }
        match /public/{all=**} {
            allow read: if true
        }
    }
}